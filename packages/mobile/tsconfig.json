{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "jsx": "react-native",
        "lib": [
            "ES2022"
        ],
        "moduleResolution": "bundler",
        "noEmit": true,
        "paths": {
            "~/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ]
}